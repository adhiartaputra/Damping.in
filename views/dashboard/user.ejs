<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <% include ../partials/bootstrap %>
    <% include ../partials/navigation_bar_user %>
    <title>user dashboard</title>
  </head>
  <body>
    <div class="container">
        <table class="table">
            <tr>
              <th>No</th>
              <th>Event Detail</th>
              <th>Partner</th>
              <th>Status</th>
            </tr>
      
            <tr>
              <td><%= 1 %></td>
              <td><%= eventDetail.event %></td>
              <td><%= partner.first_name %> <%= partner.last_name %></td>
              <% if(eventDetail.status === null) { %>
                <td>waiting</td>
              <% } else if(eventDetail.status === true) { %>
                <td>Accepted</td>
              <% } else { %>
                <td>Rejected</td>
              <% } %>
            </tr>
          </table>
    </div>
  </body>
</html>
