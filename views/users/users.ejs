<!DOCTYPE html>
<html>
  <head>
    <meta charset="UTF-8">
  </head>
  <body>
      <h1>List Users</h1>
      <h2><a href ="/user/add/">Add User</a></h2>
    <table border="">
        <tr>
          <th>ID</th>
          <th>First Name</th> 
          <th>Last Name</th> 
          <th>Gender</th>
          <th>Age</th>
          <th>Email</th>
          <th>Phone</th>
          <th>Action</th>
        </tr>
        
        <% users.sort((a,b) => a.id -b.id ).forEach((v,i) => { %>
        <tr>
            <td><%= v.id %></td>
            <td><%= v.first_name %></td>
            <td><%= v.last_name %></td>
            <td><%= v.gender %></td>
            <td><%= v.age %></td>
            <td><%= v.email %></td>
            <td><%= v.phone %></td>
            <td>
                <a href= "/user/edit/<%= v.id %>">Edit</a>
                <a href= "/user/delete/<%= v.id %>">Delete</a>
            </td>
        </tr>
        <% }) %>   
      </table>
      <footer></footer>
  </body>
</html>

